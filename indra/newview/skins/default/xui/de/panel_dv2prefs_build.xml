<panel label="Build" name="panel_dv2prefs_build">
  <text name="text_box_objprams">Default-Objektparameter</text>
        <spinner label="X:" name="X size"/>
        <spinner label="Y:" name="Y size"/>
        <spinner label="Z:" name="Z size" />
	<text name="text_box_objmaterial">Material:</text>
        <combo_box name="material"
         control_name="FSBuildPrefs_Material">
             <combo_item name="Stone" value="Stone">Stein</combo_item>
             <combo_item name="Metal" value="Metal">Metall</combo_item>
             <combo_item name="Glass" value="Glass">Glas</combo_item>
             <combo_item name="Wood" value="Wood">Holz</combo_item>
             <combo_item name="Flesh" value="Flesh">Fleisch</combo_item>
             <combo_item name="Plastic" value="Plastic">Plastik</combo_item>
             <combo_item name="Rubber" value="Rubber">Gummi</combo_item>
        </combo_box>

 <text name="text_box_pivotpoint">Angelpunkt</text>
